export CLICOLOR=1
export LSCOLORS=ExFxCxDxBxegedabagacad

export PS1="\[\033[1;94m\]\w \[\033[1;97m\]\$\[\033[0m\]"
. ~/.git-completion.bash

alias cd..='cd ..'
alias c.='cd ..'
alias c..='cd ../..'
alias c...='cd ../../..'
alias c....='cd ../../../..'
alias c.....='cd ../../../../..'
alias gti='git'
alias du='du -hs'
alias tree='tree -ahC'
alias spoj="cd ~/SPOJ"
alias codechef="cd ~/codechef"
alias checkpl="~/SPOJ/checkpatch.pl --no-tree -f "

shopt -s nocaseglob #make ls *ttl* expand to TTL Cook. To unset use shopt -u nocaseglob
